<div id="header">
  <h1>"Admin Dashboard"</h1>
</div>

<div id="links">
  <%= link_to "Merchants", admin_merchants_path %>
  <%= link_to "Invoices", admin_invoices_path %>
</div>

<div id="top_customers">
  <h2> Top Customers </h2>
  <ol>
    <% @customers.top_five_customers.each do |customer| %>
      <li><%= "#{customer.first_name} #{customer.last_name} - #{customer.transaction_count} purchases" %></li>
    <% end %>
  </ol>
</div>