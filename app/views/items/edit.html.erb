<%= form_with url: merchant_item_path(@item.merchant, @item), method: :patch, local: true, data: {turbo:false} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :unit_price %>
  <%= f.text_field :unit_price %>

  <%= f.submit %>
<% end %>
