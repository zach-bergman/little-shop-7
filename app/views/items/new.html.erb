<%= form_with model: @item, url: merchant_items_path(@merchant), local: true, data: {turbo:false} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :unit_price %>
  <%= f.text_field :unit_price %>

  <%= f.submit "Create Item" %>
<% end %>
