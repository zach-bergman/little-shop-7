<%= @merchant.name %>
<%= link_to "#{@merchant.name} Items", merchant_items_path(@merchant) %>
<%= link_to "#{@merchant.name} Invoices", merchant_invoices_path(@merchant) %>
<div id="favorite_customers">
  <% binding.pry %>
  <h1>Favorite Customers</h1>
  <ul>
    <% @top_five_customers.each do |customer| %>
      <li> <%= "#{customer.first_name} #{customer.last_name} - #{customer.number_transactions}" %> </li>
    <% end %>
  </ul>
</div>
